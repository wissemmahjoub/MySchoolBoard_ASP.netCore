{
  "buildOptions": {
    "copyToOutput": {
      "include": [ "wwwroot", "**.cshtml", "appsettings.json", "vcap-local.json" ],
      "exclude": [ "bin", "obj" ]
    },
    "debugType": "portable",
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },

  "dependencies": {
    "Microsoft.AspNetCore.Cors": "1.1.1",
    "Microsoft.AspNetCore.Diagnostics": "1.0.1",
    "Microsoft.AspNetCore.Mvc": "1.0.2",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.2",
    "Microsoft.AspNetCore.Razor.Tools": {
      "type": "build",
      "version": "1.0.0-preview4-final"
    },
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.2",
    "Microsoft.AspNetCore.StaticFiles": "1.0.1",
    "Microsoft.EntityFrameworkCore": "1.0.2",
    "Microsoft.EntityFrameworkCore.Tools": "1.0.0-preview4-final",
    "Microsoft.Extensions.Configuration.CommandLine": "1.0.1",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.1",
    "Microsoft.Extensions.Configuration.Json": "1.0.1",
    "Microsoft.Extensions.Logging": "1.0.1",
    "Microsoft.Extensions.Logging.Console": "1.0.1",
    "Microsoft.Extensions.Logging.Debug": "1.0.1",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.3"
    },
    "Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.1",
    "Newtonsoft.Json": "9.0.1",
    "Npgsql": "3.1.9",
    "Npgsql.EntityFrameworkCore.PostgreSQL": "1.0.2",
    "Npgsql.EntityFrameworkCore.PostgreSQL.Design": "1.0.2"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net452+win81"
      ]
    }
  },

  "publishOptions": {
    "include": [ "wwwroot", "web.config", "appsettings.json", "**.cshtml" ],
    "exclude": [ "bin", "obj" ]
  },

  "runtimeOptions": {
    "gcServer": true
  },

  "scripts": {
    "prebuild": [ "npm install", "gulp clean", "gulp min" ],
    "prepublish": [ "npm install", "gulp clean", "gulp min" ]
  },

  "tools": {
    "Microsoft.AspNetCore.Razor.Tools": {
      "version": "1.0.0-preview4-final",
      "imports": "portable-net45+win8+dnxcore50"
    },
    "Microsoft.EntityFrameworkCore.Tools": {
      "imports": "portable-net452+win81",
      "version": "1.0.0-preview4-final"
    }
  },

  "version": "1.0.3"
}
